             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="40"
             x:Class="client.Views.ChoreEntryView">
    <Panel>
        <TextBlock VerticalAlignment="Center" Margin="10 0">Chore Name</TextBlock>
        <DockPanel VerticalAlignment="Center"
                   HorizontalAlignment="Right">
            <TextBlock VerticalAlignment="Center">Privilege</TextBlock>
            <Button VerticalAlignment="Center" Margin="10 0">Manage</Button>
        </DockPanel>
      
    </Panel>
</UserControl>
