<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="40"
             x:Class="client.Views.TodoChoreView">
    <Panel>
        <TextBlock VerticalAlignment="Center" Margin="10 0">Name</TextBlock>
        <DockPanel HorizontalAlignment="Right">
            <TextBlock VerticalAlignment="Center" Margin="10 0 ">Time left</TextBlock>
            <CheckBox VerticalAlignment="Center"></CheckBox>
        </DockPanel>
    </Panel>
</UserControl>
